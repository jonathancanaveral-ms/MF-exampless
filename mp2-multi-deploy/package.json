{
  "name": "mp2-multi-deploy",
  "version": "1.0.0",
  "description": "A multi deployment orchestrator",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "app1:build": "nuxt build --config-file src/app1/nuxt.config.js --dotenv src/app1/.env",
